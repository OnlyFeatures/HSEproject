        </activity>
        <activity android:name=".Sign_in"/>
        <activity android:name=".Sign_in_d"/>
        <activity android:name=".Sign_in_p"/>
        <activity android:name=".Account_d"/>
        <activity android:name=".Account_p"/>
        <activity android:name=".Enter"/>
